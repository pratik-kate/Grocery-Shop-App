<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pratikcodes.ecom">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.OnlineSore">
        <activity android:name="com.pratikcodes.shopclient.MyOrdersActivity"></activity>
        <activity android:name="com.pratikcodes.shopclient.CheckOutActivity" />
        <activity android:name="com.pratikcodes.shopclient.ProductDetailActivity" />
        <activity android:name="com.pratikcodes.shopclient.ShopActivity" />
        <activity android:name="com.pratikcodes.shopclient.SignUpActivity" />
        <activity android:name="com.pratikcodes.shopclient.LoginActivity" />
        <activity android:name="com.pratikcodes.shopclient.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.pratikcodes.shopclient.MainActivity" />
        <activity android:name="com.pratikcodes.shopclient.BuyActivity" />
    </application>

</manifest>